{
  "root": "libs/clients/court-bankruptcy-cert",
  "sourceRoot": "libs/clients/court-bankruptcy-cert/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/clients/court-bankruptcy-cert/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/clients/court-bankruptcy-cert"],
      "options": {
        "jestConfig": "libs/clients/court-bankruptcy-cert/jest.config.js",
        "passWithNoTests": true
      }
    },
    "update-openapi-document": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "curl --location --request GET 'http://localhost:8081/r1/IS-DEV/GOV/10019/Domstolasyslan/getOpenAPI?serviceCode=JusticePortal-v1' --header \"X-Road-Client: $XROAD_CLIENT_ID\" > src/clientConfig.json"
        ],
        "parallel": false,
        "cwd": "libs/clients/court-bankruptcy-cert"
      }
    },
    "schemas/external-openapi-generator": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "yarn openapi-generator -o libs/clients/court-bankruptcy-cert/gen/fetch -i libs/clients/court-bankruptcy-cert/src/clientConfig.json"
        ]
      }
    }
  },
  "tags": []
}
