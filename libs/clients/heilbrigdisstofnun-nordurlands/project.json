{
  "root": "libs/clients/heilbrigdisstofnun-nordurlands",
  "sourceRoot": "libs/clients/heilbrigdisstofnun-nordurlands/src",
  "projectType": "library",
  "architect": {
    "lint": {
      "builder": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/clients/heilbrigdisstofnun-nordurlands/**/*.ts"
        ]
      }
    },
    "test": {
      "builder": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/clients/heilbrigdisstofnun-nordurlands"],
      "options": {
        "jestConfig": "libs/clients/heilbrigdisstofnun-nordurlands/jest.config.js",
        "passWithNoTests": true
      }
    },
    "update-openapi-document": {
      "builder": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "curl https://external.api.saga.dev.hc.t.is/swagger/v1/swagger.json -H 'Accept: application/json' > src/clientConfig.json"
        ],
        "parallel": false,
        "cwd": "libs/clients/heilbrigdisstofnun-nordurlands"
      }
    },
    "schemas/external-openapi-generator": {
      "builder": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "yarn openapi-generator -o libs/clients/heilbrigdisstofnun-nordurlands/gen/fetch -i libs/clients/heilbrigdisstofnun-nordurlands/src/clientConfig.json",
          "echo \"export { ApiV1AuthLoginPostRequest } from './AuthApi'\" >> libs/clients/heilbrigdisstofnun-nordurlands/gen/fetch/apis/index.ts"
        ],
        "parallel": false
      }
    }
  },
  "tags": []
}
