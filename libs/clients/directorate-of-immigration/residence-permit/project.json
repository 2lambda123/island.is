{
  "name": "clients-directorate-of-immigration-residence-permit",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/clients/directorate-of-immigration/residence-permit/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/clients/directorate-of-immigration/residence-permit/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/libs/clients/directorate-of-immigration/residence-permit"
      ],
      "options": {
        "jestConfig": "libs/clients/directorate-of-immigration/residence-permit/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "update-openapi-document": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "curl https://utl-umsokn.azurewebsites.net/swagger/v1/swagger.json -H 'Accept: application/json' > src/clientConfig.json"
        ],
        "parallel": false,
        "cwd": "libs/clients/directorate-of-immigration/residence-permit"
      }
    },
    "schemas/external-openapi-generator": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "yarn openapi-generator -o libs/clients/directorate-of-immigration/residence-permit/gen/fetch -i libs/clients/directorate-of-immigration/residence-permit/src/clientConfig.json"
        ]
      }
    }
  },
  "tags": ["lib:client", "scope:client"]
}
