{
  "openapi": "3.0.1",
  "info": {
    "title": "Húsnæðisbætur reiknivél",
    "description": "Vefþjónusta sem inniheldur reiknivél til þess að reikna út húsnæðisbætur.",
    "version": "v1"
  },
  "paths": {
    "/api/v1/Authenticate/token": {
      "post": {
        "tags": ["Authenticate"],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": { "$ref": "#/components/schemas/LoginModel" }
            },
            "text/json": {
              "schema": { "$ref": "#/components/schemas/LoginModel" }
            },
            "application/*+json": {
              "schema": { "$ref": "#/components/schemas/LoginModel" }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": { "$ref": "#/components/schemas/TokenModel" }
              },
              "application/json": {
                "schema": { "$ref": "#/components/schemas/TokenModel" }
              },
              "text/json": {
                "schema": { "$ref": "#/components/schemas/TokenModel" }
              }
            }
          }
        }
      }
    },
    "/api/v1/Reiknivel/reiknivel": {
      "post": {
        "tags": ["Reiknivel"],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "fjoldiHeimilismanna",
                  "heildarEignir",
                  "heildarTekjur",
                  "husnaedisKostnadur"
                ],
                "type": "object",
                "properties": {
                  "fjoldiHeimilismanna": {
                    "maximum": 4,
                    "minimum": 1,
                    "pattern": "^[0-9]+$",
                    "type": "integer",
                    "format": "int32"
                  },
                  "heildarTekjur": {
                    "pattern": "^[0-9]+$",
                    "type": "integer",
                    "format": "int32"
                  },
                  "heildarEignir": {
                    "pattern": "^[0-9]+$",
                    "type": "integer",
                    "format": "int32"
                  },
                  "husnaedisKostnadur": {
                    "pattern": "^[0-9]+$",
                    "type": "integer",
                    "format": "int32"
                  }
                }
              },
              "encoding": {
                "fjoldiHeimilismanna": { "style": "form" },
                "heildarTekjur": { "style": "form" },
                "heildarEignir": { "style": "form" },
                "husnaedisKostnadur": { "style": "form" }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/ReiknivelReturnModel"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ReiknivelReturnModel"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/ReiknivelReturnModel"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "LoginModel": {
        "required": ["password", "username"],
        "type": "object",
        "properties": {
          "username": { "minLength": 1, "type": "string" },
          "password": { "minLength": 1, "type": "string" }
        },
        "additionalProperties": false
      },
      "ReiknivelReturnModel": {
        "type": "object",
        "properties": {
          "manadarlegarHusnaedisbaetur": {
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "manadarlegTekjuSkerding": { "type": "number", "format": "double" },
          "manadarlegEignaSkerding": { "type": "number", "format": "double" },
          "manadarlegarHamarksBaetur": { "type": "number", "format": "double" },
          "manadarlegHusnaedisKostnadarSkerding": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "TokenModel": {
        "type": "object",
        "properties": {
          "token": { "type": "string", "nullable": true },
          "expiration": { "type": "string", "format": "date-time" }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "apiKey",
        "description": "JWT Authorization header using the Bearer scheme. \r\n\r\n Enter 'Bearer' [space] and then your token in the text input below.\r\n\r\nExample: \"Bearer 1safsfsdfdfd\"",
        "name": "Authorization",
        "in": "header"
      }
    }
  },
  "security": [{ "Bearer": [] }]
}
