    "/api/v{version}/Reiknivel/reiknivel": {
      "post": {
        "tags": ["Reiknivel"],
        "parameters": [
          {
            "name": "version",
            "in": "path",
            "required": true,
            "style": "simple",
            "schema": { "type": "string" }
          }
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "heildarEignir",
                  "heildarTekjur",
                  "husnaedisKostnadur"
                ],
                "type": "object",
                "properties": {
                  "fjoldiHeimilismanna": {
                    "maximum": 4,
                    "minimum": 1,
                    "type": "integer",
                    "format": "int32"
                  },
                  "heildarTekjur": {
                    "pattern": "^[0-9]+$",
                    "type": "integer",
                    "format": "int32"
                  },
                  "heildarEignir": {
                    "pattern": "^[0-9]+$",
                    "type": "integer",
                    "format": "int32"
                  },
                  "husnaedisKostnadur": {
                    "pattern": "^[0-9]+$",
                    "type": "integer",
                    "format": "int32"
                  }
                }
              },
              "encoding": {
                "fjoldiHeimilismanna": { "style": "form" },
                "heildarTekjur": { "style": "form" },
                "heildarEignir": { "style": "form" },
                "husnaedisKostnadur": { "style": "form" }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/ReiknivelReturnModel"
                }
              },
              "application/json": {
                "schema": {
