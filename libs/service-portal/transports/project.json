{
  "root": "libs/service-portal/transports",
  "sourceRoot": "libs/service-portal/transports/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/service-portal/transports/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/service-portal/transports"],
      "options": {
        "jestConfig": "libs/service-portal/transports/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["lib:portals-mypages", "scope:portals-mypages"]
}
