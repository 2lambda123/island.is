{
  "name": "samradsgatt",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/samradsgatt",
  "projectType": "application",
  "generators": {},
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "outputPath": "dist/apps/samradsgatt",
        "commands": [
          "yarn nx build-next samradsgatt",
          "yarn nx build-server samradsgatt"
        ]
      },
      "configurations": {
        "production": {
          "args": "--prod"
        }
      }
    },
    "build-next": {
      "executor": "@nrwl/next:build",
      "options": {
        "root": "apps/samradsgatt",
        "outputPath": "dist/apps/samradsgatt"
      },
      "configurations": {
        "production": {}
      },
      "outputs": ["{options.outputPath}"]
    },
    "build-server": {
      "executor": "@nrwl/node:webpack",
      "options": {
        "outputPath": "dist/apps/samradsgatt",
        "main": "apps/samradsgatt/server.ts",
        "tsConfig": "apps/samradsgatt/tsconfig.json",
        "maxWorkers": 2,
        "assets": []
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      },
      "outputs": ["{options.outputPath}"]
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "samradsgatt:build-server"
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "samradsgatt:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/samradsgatt"],
      "options": {
        "jestConfig": "apps/samradsgatt/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/samradsgatt/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "docker-next": {}
  },
  "tags": ["scope:react-next"]
}
