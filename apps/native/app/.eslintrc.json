{
  "extends": "../../../.eslintrc.json",
  "ignorePatterns": ["!**/*"],
  "rules": {
    "@nrwl/nx/enforce-module-boundaries": "off"
  },
  "overrides": [
    {
      "files": ["*.tsx", "*.ts"],
      "rules": {
        "@typescript-eslint/ban-ts-comment": "off"
      }
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {
        "@typescript-eslint/no-var-requires": "off"
      }
    }
  ]
}
