{
  "name": "api",
  "packageManager": "yarn@3.2.3",
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "deploy": "nx deploy",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "dev-services": "nx dev-services",
    "dev": "nx dev",
    "dev-init": "nx dev-init",
    "analyze": "cross-env ANALYZE=true nx build",
    "e2e": "nx e2e",
    "e2e-ci": "node ./scripts/e2e-ci.js",
    "codegen": "nx codegen",
    "translations": "nx translations",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:libs": "nx affected:libs",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:docker": "nx affected --target=docker",
    "affected": "nx affected",
    "security-audit": "bash -c 'yarn audit --level high ; [[ \"$?\" -lt  \"8\" ]]'",
    "license-audit": "license-checker --failOn 'GPL; AGPL; UNLICENSED' --summary",
    "generate": "nx generate",
    "get-secrets": "./scripts/get-secrets.sh",
    "create-secret": "./scripts/create-secret.sh",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "ts-node": "cross-env ts-node -r tsconfig-paths/register -O '{\\\"module\\\":\\\"commonjs\\\",\\\"esModuleInterop\\\":true}'",
    "openapi-generator": "openapi-generator generate -g typescript-fetch --additional-properties=typescriptThreePlus=true",
    "schemas": "node ./scripts/schemas.js",
    "gitbook": "node ./scripts/gitbook.js",
    "charts": "cd ./infra && yarn charts"
  },
  "dependencies": {
    "@apollo/subgraph": "2.1.1",
    "@grpc/grpc-js": "1.4.4",
    "@nestjs/apollo": "10.1.0",
    "@nestjs/microservices": "^8.0.0",
    "@nestjs/terminus": "7.0.1",
    "@nestjs/websockets": "^8.0.0",
    "auth-nest-tools": "workspace:^",
    "class-transformer/storage": "0.5.1",
    "clients": "workspace:^",
    "cms": "workspace:^",
    "cms-translations": "workspace:^",
    "eslint-plugin-local-rules": "^1.3.2",
    "feature-flags": "workspace:^",
    "file-storage": "workspace:^",
    "infra-nest-server": "workspace:^",
    "nest": "workspace:^",
    "services-user-profile": "workspace:^",
    "services-auth-public-api": "workspace:^",
    "reflect-metadata": "0.1.13",
    "ts-morph": "16.0.0"
  },
  "devDependencies": {
    "services-user-profile": "workspace:^",
    "services-auth-public-api": "workspace:^",
    "@anatine/esbuild-decorators": "0.2.19",
    "@anatine/esbuildnx": "0.2.0",
    "@angular-devkit/schematics": "13.3.2",
    "@nrwl/esbuild": "15.4.4",
    "@nrwl/eslint-plugin-nx": "13.10.5",
    "auth-api-lib": "workspace:^",
    "cache": "workspace:^",
    "clients": "workspace:^",
    "email-service": "workspace:^",
    "esbuild": "0.15.7",
    "infra-metrics": "workspace:^",
    "infra-nest-server": "workspace:^",
    "logging": "workspace:^",
    "nx": "13.10.3",
    "shared": "workspace:^"
  }
}
