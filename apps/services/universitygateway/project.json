{
  "root": "apps/services/universitygateway",
  "sourceRoot": "apps/services/universitygateway/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/services/universitygateway",
        "main": "apps/services/universitygateway/src/main.ts",
        "tsConfig": "apps/services/universitygateway/tsconfig.app.json",
        "assets": ["apps/services/universitygateway/src/assets"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/services/universitygateway/src/environments/environment.ts",
              "with": "apps/services/universitygateway/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "services-universitygateway:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/services/universitygateway/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/services/universitygateway"],
      "options": {
        "jestConfig": "apps/services/universitygateway/jest.config.js",
        "passWithNoTests": true
      }
    },
    
    "schemas/build-openapi": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "outputPath": "apps/services/universitygateway/src/openapi.yaml",
        "command": "yarn ts-node -P apps/services/universitygateway/tsconfig.app.json apps/services/universitygateway/src/buildOpenApi.ts"
      }
    }
  },
  "tags": []
}
