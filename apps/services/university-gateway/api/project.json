{
  "name": "services-university-gateway-api",
  "sourceRoot": "apps/services/university-gateway/api/src",
  "projectType": "application",
  "prefix": "services-university-gateway-api",
  "generators": {},
  "targets": {
    "build": {
      "executor": "./tools/executors/node:build",
      "options": {
        "outputPath": "dist/apps/services/university-gateway/api",
        "main": "apps/services/university-gateway/api/src/main.ts",
        "tsConfig": "apps/services/university-gateway/api/tsconfig.app.json",
        "assets": [
          "apps/services/university-gateway/api/src/assets",
          {
            "glob": "*",
            "input": "libs/email-service/src/tools/design",
            "output": "./email-service-assets"
          }
        ],
        "maxWorkers": 2
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      },
      "outputs": ["{options.outputPath}"]
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "services-university-gateway-api:build"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/services/university-gateway/api/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "apps/services/university-gateway/api/jest.config.js",
        "passWithNoTests": true
      },
      "outputs": ["coverage/apps/services/university-gateway/api"]
    },
    "schemas/build-openapi": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn nx run services-university-gateway-api:schemas/build-openapi-internal $NX_OPTIONS"
      }
    },
    "schemas/build-openapi-internal": {
      "executor": "nx:run-commands",
      "options": {
        "outputPath": "apps/services/university-gateway/api/src/openapi.yaml",
        "command": "cross-env INIT_SCHEMA=true yarn ts-node -P apps/services/university-gateway/api/tsconfig.app.json apps/services/university-gateway/api/src/buildOpenApi.ts"
      }
    },
    "docker-express": {},
    "service-service-portal-api": {}
  },
  "tags": ["scope:nest"]
}
